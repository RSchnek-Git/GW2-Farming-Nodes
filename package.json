{
  "name": "gw2_website",
  "version": "1.0.0",
  "description": "A practice website that allows a user to click on buttons and get game codes copied to their clipboards in game for convenience.",
  "main": "index.js",
  "engines": {
    "node": "= 12.11.1",
    "npm": "= 6.14.2"
  },
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "script/deploy",
    "seed": "node script/seed.js",
    "test": "NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/polyfill --require @babel/register",
    "start-dev": "NODE_ENV='development' webpack -w & NODE_ENV='development' npm run start-server",
    "start-server": "nodemon server -e html,js,scss",
    "start": "node server"
  },
  "author": "Robert Schnek",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cool-ascii-faces": "^1.3.4",
    "express": "^4.17.1",
    "git-url-parse": "^11.1.2",
    "history": "^4.10.1",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sequelize": "^5.21.5",
    "simple-git": "^3.3.0",
    "yaml": "^1.9.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
